{
	"SqliteConnectionString": "filename=data\\results\\result.db",
	"SourceName": "Innsbruck",
	"BackboneFileName": "data\\Backbone_Polygons.shp",
	"BaselineFileName": "data\\Hardbone_Baseline.shp",
	"HardboneFileName": "data\\Hardbone_Polygons.shp",
	"Aoi": [ 4350000, 2640000, 4400000, 2690000 ],
	"PartitionCount": 2,
	"WaitForUserInputAfterCompletion": false,
	"Logging": {
		"Serilog": {
			"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
			"MinimumLevel": {
				"Default": "Debug",
				"Override": {
					"Microsoft.EntityFrameworkCore": "Warning"
				}
			},
			"WriteTo": [
				{ "Name": "Console" },
				{
					"Name": "File",
					"Args": {
						"path": "logs.txt"
					}
				}
			],
			"Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
			"Properties": {
				"Application": "ClcPlusRetransformer"
			}
		}
	}
}