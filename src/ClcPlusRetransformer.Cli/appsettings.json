{
	// Input data
	"BackboneFileName": "data\\new\\BB_rasterizedVector_poly.shp",
	"BaselineFileName": "data\\new\\HB_smoothVector_lines.shp",
	"HardboneFileName": "data\\new\\HB_rasterizedVector_poly.shp",

	// Output data
	"ProcessedOutputFileName": "data\\results\\output.shp",
	"SqliteConnectionString": "filename=data\\results\\result.db",
	"SourceName": "Innsbruck",

	// Tile & Merge settings
	"PartitionCount": 2, // 1 to process as a whole, or split into N x N tiles and merge afterwards (using SQLite)
	"DegreeOfParallelism": 2, // if PartitionCount > 1, process N tiles in parallel

	// Process for Area of Interest
	// if PartitionCount > 1, Aoi is NEEDED for proper tiling
	// Remove Aoi to process the whole Backbone file extent as a whole, or use Aoi to clip before processing
	"Aoi": [ 5026780, 2674080, 5311320, 2932000 ],

	"WaitForUserInputAfterCompletion": true,
	"Logging": {
		"Serilog": {
			"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
			"MinimumLevel": {
				"Default": "Debug",
				"Override": {
					"Microsoft.EntityFrameworkCore": "Warning"
				}
			},
			"WriteTo": [
				{ "Name": "Console" },
				{
					"Name": "File",
					"Args": {
						"path": "logs.txt"
					}
				}
			],
			"Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
			"Properties": {
				"Application": "ClcPlusRetransformer"
			}
		}
	}
}